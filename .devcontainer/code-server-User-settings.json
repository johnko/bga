{
  "extensions.allowed": {},
  "chat.agent.enabled": false,
  "chat.commandCenter.enabled": false,
  "chat.disableAIFeatures": true,
  "chat.editor.fontSize": 14,
  "chat.extensionUnification.enabled": false,
  "comments.visible": false,
  "debug.console.fontSize": 14,
  "debug.terminal.clearBeforeReusing": true,
  "docsYaml.telemetry.enableTelemetry": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.foldingImportsByDefault": true,
  "editor.fontSize": 14,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.side": "left",
  "editor.minimap.size": "fit",
  "editor.renderWhitespace": "all",
  "editor.scrollOnMiddleClick": true,
  "editor.showFoldingControls": "always",
  "editor.stickyScroll.enabled": true,
  "editor.suggest.showStatusBar": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.excludeGitIgnore": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "never",
  "git.replaceTagsWhenPull": true,
  "github.copilot.chat.backgroundAgent.enabled": false,
  "github.copilot.chat.cloudAgent.enabled": false,
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.enable": {
    "*": false
  },
  "github.gitProtocol": "ssh",
  "githubPullRequests.assignCreated": "${user}",
  "githubPullRequests.createOnPublishBranch": "always",
  "githubPullRequests.defaultCommentType": "single",
  "githubPullRequests.defaultCreateOption": "createDraft",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.focusedMode": false,
  "githubPullRequests.hideViewedFiles": true,
  "githubPullRequests.ignoreSubmodules": true,
  "githubPullRequests.notifications": "pullRequests",
  "githubPullRequests.pullBranch": "always",
  "githubPullRequests.pushBranch": "always",
  "githubPullRequests.quickDiff": true,
  "githubPullRequests.showPullRequestNumberInTree": true,
  "githubPullRequests.useReviewMode": {
    "closed": false,
    "merged": false
  },
  "githubRepositories.autoFetch.interval": 180,
  "go.survey.prompt": false,
  "inlineChat.holdToSpeech": false,
  "redhat.telemetry.enabled": false,
  "remote.SSH.experimental.chat": false,
  "remoteHub.gitProtocol": "ssh",
  "scm.alwaysShowRepositories": true,
  "scm.defaultViewMode": "tree",
  "scm.defaultViewSortKey": "status",
  "scm.diffDecorationsIgnoreTrimWhitespace": "true",
  "scm.inputFontSize": 14,
  "scm.repositories.visible": 20,
  "search.defaultViewMode": "tree",
  "search.quickAccess.preserveInput": true,
  "search.searchEditor.defaultNumberOfContextLines": 3,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "settingsSync.ignoredExtensions": [
    "github.copilot-chat",
    "github.copilot"
  ],
  "task.quickOpen.skip": true,
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.hideOnStartup": "whenEmpty",
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.initialHint": false,
  "testing.automaticallyOpenPeekView": "failureAnywhere",
  "testing.automaticallyOpenTestResults": "openOnTestFailure",
  "testing.coverageToolbarEnabled": true,
  "typescript.surveys.enabled": false,
  "update.showReleaseNotes": false,
  "vs-kubernetes": {},
  "window.clickThroughInactive": false,
  "window.commandCenter": false,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.confirmSaveUntitledWorkspace": false,
  "window.newWindowDimensions": "offset",
  "window.newWindowProfile": "Default",
  "window.openFilesInNewWindow": "default",
  "window.openFoldersInNewWindow": "on",
  "window.openWithoutArgumentsInNewWindow": "on",
  "window.restoreWindows": "preserve",
  "workbench.colorTheme": "Default Dark+",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.showTabIndex": true,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.wrapTabs": true,
  "workbench.panel.defaultLocation": "top",
  "workbench.startupEditor": "none",
  "zenMode.fullScreen": false,
  "zenMode.hideActivityBar": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false
}
